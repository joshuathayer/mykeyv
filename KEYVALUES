DROP TABLE IF EXISTS KeyValue0;
DROP TABLE IF EXISTS KeyValue1;
DROP TABLE IF EXISTS KeyValue2;
DROP TABLE IF EXISTS KeyValue3;
DROP TABLE IF EXISTS KeyValue4;
DROP TABLE IF EXISTS KeyValue5;

CREATE TABLE KeyValue0 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;

CREATE TABLE KeyValue1 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;

CREATE TABLE KeyValue2 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;

CREATE TABLE KeyValue3 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;

CREATE TABLE KeyValue4 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;

CREATE TABLE KeyValue5 (
	TheKey CHAR(22) PRIMARY KEY,
	TheValue LONGBLOB
	) ENGINE InnoDB;
